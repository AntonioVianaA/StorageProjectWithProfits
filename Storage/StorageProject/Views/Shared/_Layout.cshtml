<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <link rel="apple-touch-icon" sizes="76x76" href="~Content/assets/img/apple-icon.png">
    <link rel="icon" type="image/png" href="~Content/assets/img/favicon.png">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>@ViewBag.Title</title>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport' />
    <!--     Fonts and icons     -->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700,200" rel="stylesheet" />
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet">
    <!-- CSS Files -->
    <link href="~/Content/assets/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/assets/css/paper-dashboard.css?v=2.0.0" rel="stylesheet" />
    <script src="~/Content/assets/js/core/jquery.min.js"></script>

</head>

<body class="">
    <div class="wrapper ">
        <div class="sidebar" data-color="black" data-active-color="danger">
            <!--
                Tip 1: You can change the color of the sidebar using: data-color="blue | green | orange | red | yellow"
            -->
            <div class="logo">
                <a href="http://www.creative-tim.com" class="simple-text logo-mini">
                    <div class="logo-image-small">
                        <img src="~/Content/assets/img/logo-small.png">
                    </div>
                </a>
                <a href="http://www.creative-tim.com" class="simple-text logo-normal">
                    Cake Control
                </a>
            </div>
            <div class="sidebar-wrapper">
                <ul class="nav">
                    <li class="" id="ItemHome">
                        <a href="@Url.Action("Index", "Home")">
                            <i class="nc-icon nc-bank"></i>
                            <p>Dashboard</p>
                        </a>
                    </li>
                    <li class="" id="ItemUsuario">
                        <a href="@Url.Action("Index", "Usuario")">
                            <i class="nc-icon nc-single-02"></i>
                            <p>User Profile</p>
                        </a>
                    </li>
                    <li class="" id="ItemEstoque">
                        <a href="@Url.Action("Index", "Estoque")">
                            <i class="nc-icon nc-box-2"></i>
                            <p>Table Storage</p>
                        </a>
                    </li>
                    <li class="" id="ItemReceita">
                        <a href="@Url.Action("Index", "Receita")">
                            <i class="nc-icon nc-book-bookmark"></i>
                            <p>Recipe</p>
                        </a>
                    </li>
                    <li class="" id="ItemHistorico">
                        <a href="@Url.Action("Index", "Historico")">
                            <i class="nc-icon nc-single-copy-04"></i>
                            <p>Historic</p>
                        </a>
                    </li>
                    <li>
                        <a href="./typography.html">
                            <i class="nc-icon nc-button-power"></i>
                            <p>Exit</p>
                        </a>
                    </li>
                </ul>
            </div>
        </div>
        <div class="main-panel">
            <!-- Navbar -->
            <nav class="navbar navbar-expand-lg navbar-absolute fixed-top navbar-transparent">
                <div class="container-fluid">
                    <div class="navbar-wrapper">
                        <div class="navbar-toggle">
                            <button type="button" class="navbar-toggler">
                                <span class="navbar-toggler-bar bar1"></span>
                                <span class="navbar-toggler-bar bar2"></span>
                                <span class="navbar-toggler-bar bar3"></span>
                            </button>
                        </div>
                        <a class="navbar-brand" href="./dashboard.html" style="padding-left: 50%; ">Welcome to Cake Control</a>
                    </div>

                </div>
            </nav>



            <div class="content">
                <!--Conteudo DASHBOARD -->
                @RenderBody()
            </div>


            <footer class="footer footer-black  footer-white ">
                <div class="container-fluid">
                   
                            <p style="text-align:center">
                                Desenvolvido por Andrey Masiero & Antonio Viana &copy;
                                @DateTime.Now.Year
                            </p>

                      
                </div>
            </footer>
        </div>
    </div>
    <!--   Core JS Files   -->
    <script src="~/Content/assets/js/core/jquery.min.js"></script>
    <script src="~/Content/assets/js/core/popper.min.js"></script>
    <script src="~/Content/assets/js/core/bootstrap.min.js"></script>
    <script src="~/Content/assets/js/plugins/perfect-scrollbar.jquery.min.js"></script>


    <!-- Control Center for Now Ui Dashboard: parallax effects, scripts for the example pages etc -->
    <script src="~/Content/assets/js/paper-dashboard.min.js?v=2.0.0" type="text/javascript"></script>


</body>

</html>    



 <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        